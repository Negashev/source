input {
  file {
    path => "/load/*/*"
    start_position => "beginning"
  }
}
filter {
  csv {
    separator => "	"
  }
  grok {
    match => ["path","%{GREEDYDATA}/%{GREEDYDATA:type}/%{GREEDYDATA:filename}"]
  }
  mutate
  {
    remove_field => [ "message", "@version", "path"]
  }
}
output {
  stdout { codec => rubydebug }
}
