input {
  redis {
    host => "{{ default .Env.REDIS_HOST "redis" }}"
    port => 6379
    threads => 16
    key => "logstash"
    data_type => "list"
    codec => json
    batch_count => 10000
  }
}
output {
  file {
    message_format => "%{[column1]}\t%{[column2]}\t%{[column3]}\t%{[column4]}\t%{[column5]}\t%{[column6]}\t%{[column8]}"
    path => "/load/%{filename}"
  }
}
