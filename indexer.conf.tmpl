input {
  redis {
    host => "{{ default .Env.REDIS_HOST "redis" }}"
    port => 6379
    threads => 16
    key => "logstash"
    data_type => "list"
    workers => 16
    batch => true
    batch_events => 1000
    batch_timeout => 5
  }
}
output {
  elasticsearch {
    hosts => "elasticsearch"
    index => "logstash-%{+YYYY.MM.dd.HH.mm}"
    workers => 16
  }
}
