input {
  redis {
    host => "{{ default .Env.REDIS_HOST "redis" }}"
    port => 6379
    threads => 32
    key => "logstash"
    data_type => "list"
    codec => json
    batch_count => 25000
  }
}
output {
  elasticsearch {
    hosts => "elasticsearch"
    index => "logstash-%{+YYYY.MM.dd.HH.mm}"
    workers => 32
  }
}
