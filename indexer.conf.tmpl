input {
  redis {
    host => "{{ default .Env.REDIS_HOST "redis" }}"
    port => 6379
    threads => 16
    key => "logstash"
    data_type => "list"
    codec => json
    batch_count => 10000
  }
}
output {
  file {				
    message_format => "%{[column1]}	%{[column2]}	%{[column3]}	%{[column4]}	%{[column5]}	%{[column6]}	%{[column8]}"				
    path => "/load/%{filename}_%{host}"				
    flush_interval => 5
  }
}
